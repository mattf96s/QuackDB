{
  "name": "quackdb",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.14.0",
  "repository": {
    "url": "https://github.com/mattf96s/quackdb"
  },
  "author": {
    "name": "Matt Fainman"
  },
  "description": "QuackDB is an offline-first DuckDB playground",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "sso": "aws sso login --sso-session=sso-exifbrowser",
    "set-node-v": "pnpm env use --global 20",
    "deploy-prod": "NODE_ENV=production sst deploy --stage prod --profile exifbrowser"
  },
  "devDependencies": {
    "sst": "^2.39.5",
    "aws-cdk-lib": "2.110.1",
    "constructs": "10.3.0",
    "typescript": "^5.3.3",
    "@tsconfig/node18": "^18.2.2"
  },
  "workspaces": [
    "packages/*",
    "packages/web/*"
  ]
}