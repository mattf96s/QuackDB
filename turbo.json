{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL_ENV"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ]
    },
    "dev": {
      "dependsOn": [
        "^rebuild:workers"
      ],
      "cache": false,
      "persistent": true
    },
    "clean": {},
    "rebuild:workers": {
      "dependsOn": [
        "clean:workers",
        "build:workers"
      ],
      "outputs": [
        "public/worker-dist/**"
      ]
    }
  }
}