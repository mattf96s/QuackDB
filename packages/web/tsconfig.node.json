{
  "compilerOptions": {
    "types": [
      "@types/wicg-file-system-access"
    ],
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./app/*"
      ]
    },
    "skipLibCheck": false,
    "noEmit": true,
    "allowImportingTsExtensions": true,
  },
  "include": [
    "vite.config.ts"
  ]
}